<div class="list_holder">
  <div class="w100"><%= link_to "remove", template_specification, :confirm => 'Are you sure want to remove this division?', :method => :delete , :class => "remove"%></div>
  <div class="list_number"><%=h report_article_number(template_specification.division.short_number) %></div>
  <div class="list_name"><%= link_to template_specification.division.name, add_template_multiple_sections_path(:template_job_id => @template_job.id, :template_specification_id => template_specification.id) %></div>
  <div class="list_hold">
    <%  template_specification.template_specification_sections.each do |spec_sec| %>
      <div class="list_container">
        <div class="serial"><%=  spec_sec.section.modif_number %></div>
        <div class="tmp-name"><%= link_to(spec_sec.section.name,"#", :class=> "specSectionName") %></div>
        <div class="icon"><%= link_to image_tag("/images/minus.png"), spec_sec, :confirm => 'Are you sure want to remove this section?', :method => :delete %></div>
      </div>
    <%end%>
	</div>
</div>