<% form_for @document do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :specification_section_id %><br />
    <%= f.text_field :specification_section_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p><%= f.submit %></p>
<% end %>
